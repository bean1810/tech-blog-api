        "dotenv": "^16.0.1",
        "express": "^4.18.1",
        "path": "^0.12.7",
        "pg": "^8.8.0",
        "pg-hstore": "^2.3.4",
        "sequelize": "^6.21.4"
    },
    "devDependencies": {
        "@babel/cli": "^7.18.10",
        "@babel/core": "^7.18.13",
        "@babel/node": "^7.18.10",
        "@babel/plugin-transform-runtime": "^7.18.10",
        "@babel/preset-env": "^7.18.10",
        "@babel/register": "^7.18.9",
        "@babel/runtime": "^7.18.9",
        "babel-loader": "^8.2.5",
