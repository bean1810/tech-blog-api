{
    "name": "tech-blog-api",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "dev": "nodemon --exec babel-node ./api/index.js",
        "test": "export NODE_ENV=test &&  sequelize db:migrate:undo:all  && sequelize db:migrate  && nyc --require @babel/register  mocha ./api/test/* --timeout 20000 --exit",
        "build": "rm -rf ./build && babel -d ./build ./api -s",
        "generate-lcov": "nyc report --reporter=text-lcov > lcov.info",
        "coveralls-coverage": "coveralls < lcov.info",
        "codeclimate-coverage": "codeclimate-test-reporter < lcov.info",
        "coverage": "nyc npm test && npm run generate-lcov && npm run coveralls-coverage && npm run codeclimate-coverage",
        "migrate-test": "NODE_ENV=test node migrate.js up"
    },
    "keywords": [],
